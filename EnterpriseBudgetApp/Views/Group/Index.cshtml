@model IEnumerable<EnterpriseBudgetApp.Models.Group>
@Scripts.Render("~/bundles/jqueryui")
@{
    ViewBag.Title = "Index";
}
<script>
    $(function () {
        $("#tabs").tabs().addClass("ui-tabs-vertical ui-helper-clearfix");
        $("#tabs li").removeClass("ui-corner-top").addClass("ui-corner-left");
    });
</script> 
<div id="tabs">
  <ul>
    <li><a href="#tabs-1">Membership</a></li>
    <li><a href="#tabs-2">Budget</a></li>
    <li><a href="#tabs-3">Recent Transactions</a></li>
  </ul>
  <div id="tabs-1">
    <fieldset>
    <legend>Group</legend>
    <div class="display-label">
        <p>Current Members: Work in progress</p>
        @foreach (var item in Model) 
        {
            Html.RenderPartial("~/Views/Group/_GroupUsers.cshtml", item);
        }
    </div>
</fieldset>
  </div>
  <div id="tabs-2" class="ui-tabs-hide">
      <p>Budget Breakdown: Work in progress</p>
  </div>
  <div id="tabs-3" class="ui-tabs-hide">
    <p>Recent Transactions: Work in progress</p>
  </div>
</div>